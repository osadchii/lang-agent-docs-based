{
  "id": null,
  "uid": "backend-observability",
  "title": "Backend Observability",
  "version": 1,
  "timezone": "browser",
  "schemaVersion": 39,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({service=\"backend\"} |= \"access\" | json | __error__=\"\" [1m]))",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "p95 latency (ms)",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.95, {service=\"backend\"} |= \"access\" | json | unwrap duration_ms [5m])",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "4xx rate (req/min)",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({service=\"backend\"} |= \"access\" | json | status_code >= 400 and status_code < 500 [5m])) * 60",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/min",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 5
              },
              {
                "color": "red",
                "value": 15
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 7
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "5xx rate (req/min)",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({service=\"backend\"} |= \"access\" | json | status_code >= 500 [5m])) * 60",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/min",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 7
      }
    },
    {
      "id": 5,
      "type": "table",
      "title": "Top endpoints (last 5m)",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, sum(rate({service=\"backend\"} |= \"access\" | json | __error__=\"\" [5m])) by (http_path))",
          "resultFormat": "table",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center"
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true
      },
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 12,
        "y": 7
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "LLM tokens per second (by operation)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (operation) (rate(app_llm_total_tokens_total[5m]))",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 7,
        "w": 14,
        "x": 0,
        "y": 12
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "LLM spend (USD/hour)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(app_llm_estimated_cost_usd_total[5m])) * 3600",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 0.05
              },
              {
                "color": "red",
                "value": 0.1
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "alert": {
        "alertRuleTags": {},
        "conditions": [
          {
            "evaluator": {
              "params": [
                0.05
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ],
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "executionErrorState": "alerting",
        "for": "2m",
        "frequency": "1m",
        "handler": 1,
        "message": "LLM hourly spend exceeded $0.05",
        "name": "LLM spend budget",
        "noDataState": "no_data",
        "notifications": []
      },
      "gridPos": {
        "h": 7,
        "w": 10,
        "x": 14,
        "y": 12
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "10s",
      "30s",
      "1m",
      "5m"
    ]
  }
}
